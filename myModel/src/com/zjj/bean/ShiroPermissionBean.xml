<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class table="shiro_role" name="com.zjj.bean.ShiroRoleBean" dynamic-insert="true" dynamic-update="true">
		<id name="roleId" column="role_id" type="int">
			<generator class="native"></generator>
		</id>
		<property name="description"></property>
		<property name="createTime" column="create_time" type="java.util.Date" ></property>
		<property name="createUser" column="create_user"></property>
		<property name="status"></property>
		<property name="builtin"></property>
	</class>
</hibernate-mapping>