<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class table="user" name="com.zjj.bean.UserBean" dynamic-insert="true" dynamic-update="true">
		<id name="userId" column="user_id" type="int">
			<generator class="native"></generator>
		</id>
		<property name="account"></property>
		<property name="userName" column="user_name"></property>
		<property name="password"></property>
		<property name="age" type="int"></property>
		<property name="userType" column="user_type" type="int"></property>
	</class>
</hibernate-mapping>